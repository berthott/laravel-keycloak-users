{
  "name": "berthott/laravel-keycloak-users",
  "description": "Laravel user administration in Keycloak",
  "license": "MIT",
  "authors": [
      {
          "name": "Jan Bladt",
          "email": "berthott@web.de"
      }
  ],
  "autoload" : {
      "psr-4" : {
          "berthott\\KeycloakUsers\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "berthott\\KeycloakUsers\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "berthott\\KeycloakUsers\\KeycloakUsersServiceProvider"
          ]
      }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "doctrine/dbal": "^3.3"
  },
  "require": {
    "guzzlehttp/guzzle": "^7.3",
    "berthott/laravel-crudable": "^3.0",
    "haizad/laravel-keycloak-admin": "^1.9"
  }
}
